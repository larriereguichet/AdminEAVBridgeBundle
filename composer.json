{
  "name": "lag/admin-eav-bridge-bundle",
  "description": "Bridge between LAG\\AdminBundle and Sidus\\EAVModelBundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "johnkrovitch",
      "email": "arnaudfrezet@gmail.com"
    }
  ],
  "require": {
    "lag/adminbundle": "v0.4",
    "sidus/eav-model-bundle": "dev-symfony3"
  },
  "autoload": {
    "psr-0": {
      "LAG\\AdminEAVBridgeBundle": ""
    }
  },
  "target-dir": "LAG/AdminEAVBridgeBundle",
  "config": {
    "bin-dir": "bin"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
